{
  "name": "ihos/laravel-totem",
  "description": "A Laravel package to manage your cron jobs through a beautiful dashboard",
  "keywords": [
    "laravel",
    "schedule",
    "manager",
    "Code Studio",
    "totem",
    "ihos"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Vinicius Assis Nascimento",
      "email": "viniciusassisnascimento@gmail.com"
    }
  ],
  "require": {
    "php": "^7.4",
    "doctrine/dbal": "^2.11",
    "illuminate/bus": "^8.11",
    "illuminate/console": "^8.11",
    "illuminate/contracts": "^8.11",
    "illuminate/database": "^8.11",
    "illuminate/events": "^8.11",
    "illuminate/notifications": "^8.11",
    "laravelcollective/html": "^6.0"
  },
  "require-dev": {
    "mockery/mockery": "^1.0",
    "orchestra/database": "^6.0",
    "orchestra/testbench" : "^6.0",
    "phpunit/phpunit": "^9.0"
  },
  "suggest": {
    "nexmo/client": "Required for sms notifications."
  },
  "autoload": {
    "psr-4": {
      "Studio\\Totem\\": "src/",
        "Studio\\Totem\\Tests\\": "tests/",
        "Database\\Factories\\": "database/factories/"
    }
  },
  "extra": {
    "component": "package",
    "branch-alias": {
      "dev-8.0": "8.1.x-dev"
    },
    "laravel": {
      "providers": [
        "Studio\\Totem\\Providers\\TotemServiceProvider"
      ]
    }
  },
  "config": {
    "sort-packages": true
  },
  "prefer-stable": true,
  "minimum-stability": "dev"
}
